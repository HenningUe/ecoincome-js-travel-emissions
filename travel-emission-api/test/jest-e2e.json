{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/enum-helper": "<rootDir>/../libs/enum-helper/src",
    "@app/enum-helper/(.*)": "<rootDir>/../libs/enum-helper/src/$1",
    "@app/travel-emission-calc": "<rootDir>/../libs/travel-emission-calc/src",
    "@app/travel-emission-calc/(.*)": "<rootDir>/../libs/travel-emission-calc/src/$1"
  }
}