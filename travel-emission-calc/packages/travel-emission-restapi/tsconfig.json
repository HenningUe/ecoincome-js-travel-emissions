{
  "extends": "../../tsconfig.base.json",
  "references": [
    {
      "path": "../travel-emission"
    }
  ],
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "../travel-emission": ["../travel-emission/src"],
    },
    "plugins": [
      {
        "name": "@nestjs/swagger"
      }
    ],
  }
}
